<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ADVANCE_JAVA</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>print_hello</description>
    <display-name>Hello</display-name>
    <servlet-name>Hello</servlet-name>
    <servlet-class>com.controller.Hello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DemoController</display-name>
    <servlet-name>DemoController</servlet-name>
    <servlet-class>com.controller.DemoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoController</servlet-name>
    <url-pattern>/democontroller</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LifeCycleMethod</display-name>
    <servlet-name>LifeCycleMethod</servlet-name>
    <servlet-class>com.controller.LifeCycleMethod</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LifeCycleMethod</servlet-name>
    <url-pattern>/lcm</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserThread</display-name>
    <servlet-name>UserThread</servlet-name>
    <servlet-class>com.controller.UserThread</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserThread</servlet-name>
    <url-pattern>/userthread</url-pattern>
  </servlet-mapping>
</web-app>